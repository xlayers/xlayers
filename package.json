    "@types/node": "~11.9.2",
    "@types/sketchapp": "^1.0.0",
    "codelyzer": "~4.5.0",
    "conventional-changelog-cli": "^2.0.11",
    "cypress": "^3.1.5",
    "jest": "^24.1.0",
    "jest-preset-angular": "^6.0.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^4.7.1",
    "puppeteer": "^1.12.2",
    "speed-measure-webpack-plugin": "^1.3.0",
    "terser": "^3.16.1",
    "ts-node": "~8.0.2",
