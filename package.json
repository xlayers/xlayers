{
  "name": "xlayers",
  "version": "1.0.0-beta.5",
  "scripts": {
    "ng": "ng",
    "--dev--": "",
    "start": "ng serve --configuration hmr",
    "prestart": "npm run build.library.all",
    "--build--": "",
    "app.build.dev": "ng build",
    "app.build.prod": "ng build --prod",
    "app.build.stamp": "bash scripts/stamp-build.bash",
    "build.library.all": "ts-node ./scripts/build-all-librarys.ts",
    "build": "run-s -l build.library.all app.build.prod app.build.stamp",
    "--test--": "",
    "test": "jest",
    "test.update": "jest --updateSnapshot tests/integration",
    "test.ci": "jest --ci --detectOpenHandles",
    "cy.open": "cypress open",
    "cy.run": "cypress run",
    "e2e": "start-server-and-test start http-get://localhost:4200 cy.run",
    "--lint--": "",
    "lint": "ng lint",
    "lint.fix": "ng lint --fix",
    "--deploy--": "",
    "deploy.prod": "firebase deploy --only hosting",
    "deploy.prod.ci.setup": "firebase use $PROJECT_ID --token $FIREBASE_TOKEN",
    "deploy.prod.ci": "firebase deploy --only hosting -m 'CI deploy' --token $FIREBASE_TOKEN",
    "deploy.ci": "run-s -l deploy.prod.ci.setup deploy.prod.ci",
    "deploy": "run-s -l lint test build deploy.prod",
    "--release--": "",
    "release": "release-it --config .release-it.json",
    "--misc--": "",
    "quality": "run-s -l lint test",
    "changelog": "conventional-changelog -p angular -u | tail -n +3",
    "changelog.write": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postchangelog.write": "node scripts/clean-changelog.js",
    "changelog.full": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postchangelog.full": "node scripts/clean-changelog.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.11",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.11",
    "@angular/compiler": "^8.2.11",
    "@angular/core": "^8.2.11",
    "@angular/forms": "^8.2.11",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.11",
    "@angular/platform-browser-dynamic": "^8.2.11",
    "@angular/router": "^8.2.11",
    "@angular/service-worker": "^8.2.11",
    "@ngxs/router-plugin": "^3.5.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/store": "^3.5.1",
    "@stackblitz/sdk": "^1.3.0",
    "big-integer": "^1.6.41",
    "buffer": "^5.2.1",
    "core-js": "^2.6.4",
    "file-saver": "^2.0.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.14.2",
    "jszip": "^3.1.5",
    "ngx-color": "^2.0.5",
    "ngx-highlightjs": "^3.0.1",
    "param-case": "^2.1.1",
    "pascal-case": "^2.0.1",
    "rxjs": "^6.4.0",
    "start-server-and-test": "^1.7.12",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "ora": "^4.0.2",
    "@angular-devkit/build-angular": "~0.803.12",
    "@angular-devkit/build-ng-packagr": "^0.803.12",
    "@angular/cli": "~8.3.12",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@angularclass/hmr": "^2.1.3",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@ngxs/devtools-plugin": "^3.5.1",
    "@ngxs/logger-plugin": "^3.5.1",
    "@types/cypress": "^1.1.3",
    "@types/file-saver": "^2.0.0",
    "@types/highlight.js": "^9.12.3",
    "@types/node": "^11.9.6",
    "@types/sketchapp": "^1.0.3",
    "codelyzer": "~5.0.0",
    "conventional-changelog-cli": "^2.0.12",
    "cypress-file-upload": "^3.0.6",
    "cypress-testing-library": "^2.3.6",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-preset-angular": "^6.0.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^5.7.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^1.12.2",
    "release-it": "^12.3.5",
    "speed-measure-webpack-plugin": "^1.3.0",
    "terser": "^3.16.1",
    "ts-node": "~8.0.2",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "3.4.5",
    "wepback": "^1.0.0"
  }
}
